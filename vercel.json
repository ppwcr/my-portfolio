{
  "version": 2,
  "builds": [
    {
      "src": "main_vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main_vercel.py"
    }
  ],
  "functions": {
    "main_vercel.py": {
      "maxDuration": 300
    }
  },
  "env": {
    "PYTHONPATH": "."
  }
}
